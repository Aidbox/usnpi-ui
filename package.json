{
  "name": "usnpi-ui",
  "version": "1.0.0",
  "description": "US NPI practitioner and organization search demo",
  "main": "index.js",
  "scripts": {
    "start": "export $(cat .env) && parcel index.html",
    "build": "rm -Rf dist && export $(cat .env) && NODE_ENV=production parcel build index.html",
    "dev": "export $(cat .env) && nodemon dev.js",
    "size": "size-limit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Mikhail Pravilenko <zorkijofficial@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "aidbox": "^2.0.0-beta.0",
    "antd": "^3.20.1",
    "date-fns": "^1.30.1",
    "gh-pages": "^2.0.1",
    "history": "^4.9.0",
    "bootstrap": "^4.3.1",
    "qs": "^6.7.0",
    "react": ">=16.8.0",
    "react-bootstrap": "^1.0.0-beta.10",
    "react-dom": ">=16.8.0",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.0",
    "nodemon": "^1.19.1",
    "parcel-bundler": "^1.12.3",
    "postcss-modules": "^1.4.1",
    "size-limit": "^1.3.7"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "flexbox": "no-2009"
      }
    }
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "size-limit": [
    {
      "webpack": false,
      "path": "dist/*.js"
    }
  ]
}
